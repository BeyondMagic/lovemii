[font]
name = Noto Sans CJK JP
size = 12

[general]
app_id = swayimg
overlay = no

[list]
all = yes

[keys.viewer]
Delete = exec rm "%"; skip_file
Ctrl+p = exec fork.nu "'%' | ^wl-copy"
Ctrl+c = exec fork.nu "cat '%' | ^wl-copy"
Ctrl+k = exec fork.nu "'%' | organize-fav.nu"
Ctrl+r = exec fork.nu "rnote '%'"
Ctrl+g = exec fork.nu "gimp %"
Ctrl+w = exec swww img "%"
Shift+greater = flip_vertical
